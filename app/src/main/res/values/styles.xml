    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorHint">@android:color/black</item>
        <item name="android:textColorHighlight">@android:color/black</item>
        <item name="android:textColorLink">@android:color/black</item>
    </style>

    <style name="SmallRatingBar">
        <item name="android:scaleX">0.4</item>
        <item name="android:scaleY">0.4</item>
        <item name="colorControlActivated">@color/PrimaryColor</item>
    </style>

    <style name="OrangeRatingBar">
        <item name="colorControlActivated">@color/PrimaryColor</item>
    </style>

