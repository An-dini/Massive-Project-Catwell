    <style name="AppTheme" parent="ThemeOverlay.AppCompat">
        <!-- ... -->
        <item name="popupTheme">@style/AppTheme.PopupOverlay</item>
        <item name="actionBarTheme">@style/AppTheme.AppBarOverlay</item>
        <!-- ... -->
    </style>

